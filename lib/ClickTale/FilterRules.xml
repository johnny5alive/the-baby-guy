		<Rule>
			<Name>Empty comment region with &lt;!-- ClickTaleExcludeBlock --&gt;</Name>
			<Find><![CDATA[/<!--[ ]*ClickTaleExcludeBlock[ ]*-->.*?<!--[ ]*EndClickTaleExcludeBlock[ ]*(.*?)-->/is]]></Find>
